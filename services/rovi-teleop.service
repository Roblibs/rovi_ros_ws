[Unit]
Description=ROVI stack: teleop
Requires=rovi-gateway.service
After=rovi-gateway.service
PartOf=rovi-gateway.service

[Service]
Type=simple
User=wass
WorkingDirectory=/home/wass/dev/rovi_ros_ws
EnvironmentFile=/home/wass/dev/rovi_ros_ws/.env.robot
RuntimeDirectory=rovi
ExecStart=/home/wass/dev/rovi_ros_ws/services/run_stack.sh teleop
KillSignal=SIGINT
TimeoutStopSec=20
Restart=no

[Install]
WantedBy=multi-user.target
